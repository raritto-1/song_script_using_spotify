{
  "version": 2,
  "builds": [
      {
          "src": "wsgi.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "wsgi.py"
      }
  ],
  "env": {
      "CLIENT_ID": "@client_id",
      "CLIENT_SECRET": "@client_secret"
  }
}

  