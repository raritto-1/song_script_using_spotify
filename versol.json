{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": { "entrypoint": "app.py" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
    "Client_id": "@Client_id",
    "Client_secret": "@Client_secret"
}

  }
  