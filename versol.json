{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": { "entrypoint": "app.py" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "CLIENT_ID": "@client_id",
      "CLIENT_SECRET": "@client_secret"
    }
  }
  